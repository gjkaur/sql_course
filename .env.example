# SQL Mastery Course - Environment Variables
# Copy this file to .env and set your values

# PostgreSQL
POSTGRES_USER=sqlcourse
POSTGRES_DB=sqlcourse
DB_PASSWORD=sqlcourse

# Trust auth enabled: no password needed for DBeaver/pgAdmin (leave password blank or use any value)
# Host: localhost | Port: 5432 | Database: sqlcourse | User: sqlcourse

# pgAdmin (optional, use --profile tools)
PGADMIN_PASSWORD=sqlcourse
PGADMIN_DEFAULT_EMAIL=admin@sqlcourse.local
